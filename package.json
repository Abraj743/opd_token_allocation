{
  "name": "opd-token-allocation-engine",
  "version": "1.0.0",
  "description": "Hospital OPD Token Allocation Engine with priority-based allocation and dynamic reallocation",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pbt": "jest --testNamePattern='Property'",
    "test:sequential": "node src/test/run-sequential-tests.js",
    "test:foundation": "node src/test/run-sequential-tests.js --foundation",
    "test:services": "node src/test/run-sequential-tests.js --services",
    "test:integration": "node src/test/run-sequential-tests.js --integration",
    "test:end-to-end": "node src/test/run-sequential-tests.js --end-to-end",
    "test:all-guided": "node src/test/run-sequential-tests.js --all",
    "test:structure": "node src/test/run-sequential-tests.js --structure",
    "test:comprehensive": "node comprehensive-opd-test.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "hospital",
    "opd",
    "token",
    "allocation",
    "healthcare",
    "appointment"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "awilix": "^10.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mongoose": "^8.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "faker": "^5.5.3",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
